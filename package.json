{
  "name": "eslint-plugin-isort",
  "description": "",
  "license": "ISC",
  "author": "Jeff <rustyangel@gmail.com> (https://tidaltheory.co)",
  "homepage": "https://github.com/stormwarning/eslint-plugin-isort",
  "repository": "stormwarning/eslint-plugin-isort",
  "version": "1.0.0",
